SELECT 
    t.extension AS `homePhone%`,
    SUBSTRING_INDEX(SUBSTRING_INDEX(t.destination, ',', 2), ',', -1) AS `telephoneNumber%`,
    d.description AS `Username`
FROM 
    incoming AS t
LEFT JOIN 
    devices AS d
ON 
    SUBSTRING_INDEX(SUBSTRING_INDEX(t.destination, ',', 2), ',', -1) = d.id
WHERE 
    t.destination LIKE '%,%,%';
